<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plonetheme.jqueryui">

    <configlet
        title="JQueryUI theme settings"
        action_id="plonetheme.jqueryui"
        appId="plonetheme.jqueryui"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@plonetheme.jqueryui-controlpanel"
        icon_expr="string:${portal_url}/plonetheme_jqueryui.jpg"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
