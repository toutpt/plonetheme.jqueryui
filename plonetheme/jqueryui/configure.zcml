<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:cmf="http://namespaces.zope.org/cmf"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="plonetheme.jqueryui">

  <include package="plone.autoform" />
  <include package="plone.resource" />
  <include package="plone.app.registry"/>
  <include package="plone.app.theming"/>
  <include package="plone.transformchain"/>
  <include package="collective.z3cform.colorpicker"/>
  <five:registerPackage package="." initialize=".initialize" />
  <cmf:registerDirectory name="plonetheme_jqueryui" />

  <adapter factory=".transform.JQueryUITransform" name="plonetheme.jqueryui.transform" />

  <genericsetup:registerProfile
      name="default"
      title="plonetheme.jqueryui"
      directory="profiles/default"
      description="Installs the plonetheme.jqueryui package"
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <browser:page
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    name="plonetheme.jqueryui-controlpanel"
    class=".controlpanel.MainControlPanelView"
    permission="cmf.ManagePortal"
    />
  <browser:page
    for="Products.CMFPlone.interfaces.IPloneSiteRoot"
    name="plonetheme.jqueryui-custom-controlpanel"
    class=".controlpanel.CustomControlPanelView"
    permission="cmf.ManagePortal"
    />
  <browser:page
    for="*"
    name="jqueryui-import-theme"
    class=".controlpanel.ImportThemeForm"
    template="controlpanel_import.pt"
    permission="cmf.ManagePortal"
    />

  <subscriber handler=".css.handle_registry_modified" />

</configure>
